<?xml version="1.0"?>
<CLBConfig version="2.0" output="output/" permissive="true">
	<Geometry nx="64" ny="32" nz="64">
		<MRT>
			<Box/>
		</MRT>
		<Wall mask="ALL" name="Lower">
			<Box ny="2"/>
		</Wall>
	</Geometry>
	<Model>
		<Param name="CenterZ" value="32"/>
		<Param name="CenterY" value="0"/>
		<Param name="CenterX" value="32"/>
		<Param name="Radius" value="12"/>
		<Param name="Viscosity_h" value="0.166"/>
		<Param name="Viscosity_l" value="0.0166"/>
		<Param name="sigma" value="5e-3"/>
		<Param name="M" value="0.05"/>
		<Param name="IntWidth" value="4"/>
		<Param name="radAngle" value="120d"/>
		<Param name="Density_l" value="0.01"/>
		<Param name="Density_h" value="1"/>
	</Model>
	<VTK what="Rho,PhaseField,U"/>
	<VTK what="Rho,PhaseField,U" Iterations="500"/>
	<Failcheck Iterations="100"/>
	<Log Iterations="20"/>
	<Solve Iterations="500"/>
</CLBConfig>
